{
  "name": "@omniboard/analyzer",
  "version": "2.24.4",
  "description": "",
  "main": "dist/index.js",
  "private": false,
  "engines": {
    "node": ">=12.0.0"
  },
  "bin": {
    "omniboard": "./dist/index.js"
  },
  "scripts": {
    "start": "node -r ts-node/register lib/index.ts",
    "build": "tsc",
    "watch": "tsc --watch",
    "format:test": "prettier lib/**/*.{ts,json}",
