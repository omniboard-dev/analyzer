    "got": "^11.8.2",
    "jsonpath-plus": "^6.0.1",
    "listr2": "^3.14.0",
    "strip-json-comments": "^3.1.1",
    "strip-json-trailing-commas": "^1.1.0",
    "xml2js": "^0.4.23",
    "xmldom": "^0.6.0",
    "xpath": "0.0.32",
    "yargs": "^17.1.1"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.2",
    "@types/xml2js": "^0.4.9",
    "@types/xmldom": "^0.1.31",
    "@types/yargs": "^17.0.2",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
