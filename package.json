    "release": "npm run format:write && npm run build && standard-version -a && git push --follow-tags origin master && npm publish --access public"
  },
  "dependencies": {
    "@types/listr": "^0.14.4",
    "chalk": "^4.1.2",
    "enquirer": "^2.3.6",
    "filesize": "^8.0.0",
